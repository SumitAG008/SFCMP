{
  "_version": "1.58.0",
  "sap.app": {
    "id": "com.sap.sf.compensation",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}"
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "rootView": {
      "viewName": "com.sap.sf.compensation.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    },
    "dependencies": {
      "minUI5Version": "1.60.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.table": {},
        "sap.ui.layout": {}
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "com.sap.sf.compensation.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "name": "compensation",
          "pattern": "",
          "target": "compensation"
        },
        {
          "name": "workflowBuilder",
          "pattern": "workflow-builder",
          "target": "workflowBuilder"
        },
        {
          "name": "workflow",
          "pattern": "workflow",
          "target": "workflow"
        }
      ],
      "targets": {
        "compensation": {
          "viewName": "CompensationWorksheet",
          "viewId": "CompensationWorksheet",
          "viewLevel": 1
        },
        "workflowBuilder": {
          "viewName": "WorkflowBuilder",
          "viewId": "WorkflowBuilder",
          "viewLevel": 1
        },
        "workflow": {
          "viewName": "WorkflowVisualization",
          "viewId": "WorkflowVisualization",
          "viewLevel": 1
        }
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.sap.sf.compensation.i18n.i18n"
        }
      },
      "compensation": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {
          "defaultBindingMode": "TwoWay"
        }
      }
    }
  }
}
